{
  "name": "license-me",
  "version": "1.0.0-canary.0",
  "description": "Create a license for your project",
  "repository": "bukinoshita/license-me",
  "author": "Bu Kinoshita <bukinoshita@gmail.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "build": "tsc",
    "test": "ava",
    "lint": "xo"
  },
  "files": [
    "dist"
  ],
  "types": "dist",
  "keywords": [
    "license",
    "license-me",
    "mit",
    "github"
  ],
  "devDependencies": {
    "@sindresorhus/tsconfig": "^0.5.0",
    "@types/fs-extra": "^8.0.0",
    "ava": "^2.4.0",
    "eslint-config-prettier": "^6.4.0",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "prettier": "^1.18.2",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.4",
    "xo": "^0.25.3"
  },
  "xo": {
    "extends": [
      "prettier"
    ],
    "extensions": [
      "ts"
    ]
  },
  "ava": {
    "babel": false,
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "files": [
      "test/**"
    ]
  },
  "lint-staged": {
    "*.js": [
      "prettier --semi false --single-quote --write",
      "yarn lint",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "fs-extra": "^8.1.0"
  }
}
